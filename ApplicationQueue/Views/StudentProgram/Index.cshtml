@model IEnumerable<ApplicationQueue.Models.StudentProgram>

@{
    ViewData["Title"] = "View";
}

<h1>View</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TeamName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Src)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsRunning)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TeamName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Src)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsRunning)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { item.Id /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Details", "Details", new { item.Id /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Delete", "Delete", new { item.Id /* id=item.PrimaryKey */ })
            </td>
        </tr>
}
    </tbody>
</table>
